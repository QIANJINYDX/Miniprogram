/* pages/index/index.wxss */
.main-top{
  display: flex;
  flex-direction: column;
  width: 100%;
  height: 110vh;
  background-size: cover;
  background-image: url('data:img/jpg;base64,');
}
.img
{
  width: 100%; 
  /* height: 100%; */
}
page{
  width:100%;
  height:100%;
  overflow: scroll;
}
.main-top .classify{
  width: 100%;
  height: 100%;
  display: flex;
  flex-direction: column;
  border: 1px solid rgb(230, 167, 96);
  border-left: 0rpx;
  border-right: 0rpx;
}
.main-top .classify text{
  margin-top: 12px;
  margin-left: 22px;
}
.wheater_image{
  height: 30px;
  width: 30px;
  /* margin-left: 25%; */
}
.main-top .classify .classify_navigation
{
  display: flex;
  align-content: center;
  padding-top: 48rpx;
}
.navigation
{
  /* margin-left: 8.33%;
  margin-right:8.33%;
  margin-top: 5%; */
  width: 33.3%;
  height: auto;
  align-content: center;
  text-align: center;
}
.main-top .classify .classify_navigation .navigation text{
  margin: 0;
}
.buttunkefu{
  width: 20rpx;
  height: 64rpx;
  border-radius: 999rpx;
}
.kefu{
  width: 20rpx;
  height: 64rpx;
}
.big_navigation{
  font-size: large;
}
button::after{
